@model CalzadoERP.Model.Orden

@{
    ViewData["Title"] = "Delete";
}

<h3>Estas seguro de eliminarlo?</h3>
<div>
    <h4>Orden</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FechaCreacionOrden)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaCreacionOrden)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FechaEntregaOrden)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaEntregaOrden)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.StatusOrden)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.StatusOrden)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FechaCierreOrden)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaCierreOrden)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdClienteNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdClienteNavigation.IdCliente)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdOrden" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Cancelar</a>
    </form>
</div>
