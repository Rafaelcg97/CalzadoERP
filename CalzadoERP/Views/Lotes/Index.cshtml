@model IEnumerable<CalzadoERP.Model.Lote>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CantidadLote)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PiezasTerminadasLote)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdEstiloNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdOrdenNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdZapateroNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CantidadLote)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PiezasTerminadasLote)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdEstiloNavigation.IdEstilo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdOrdenNavigation.IdOrden)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdZapateroNavigation.IdZapatero)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdLote">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdLote">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdLote">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
