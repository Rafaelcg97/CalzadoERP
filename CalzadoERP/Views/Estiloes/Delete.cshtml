@model CalzadoERP.Model.Estilo

@{
    ViewData["Title"] = "Delete";
}



<h3>Estas seguro de eliminarlo?</h3>
<div>

    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NombreEstilo)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NombreEstilo)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ColorEstilo)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ColorEstilo)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.GeneroEstilo)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.GeneroEstilo)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.PrecioEstilo)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.PrecioEstilo)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ComentariosEstilo)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ComentariosEstilo)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdEstilo" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a class="px-3" asp-action="Index">Cancelar</a>
    </form>
</div>
