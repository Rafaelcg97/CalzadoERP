            <td>
                @Html.DisplayFor(modelItem => item.NombreZapatero)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DireccionZapatero)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdentificacionZapatero)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EstadoZapatero)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaAsociacionZapatero)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaTerminacionZapatero)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdZapatero">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdZapatero">Details</a> |
