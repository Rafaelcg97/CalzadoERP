@model CalzadoERP.Model.Proveedor

@{
    ViewData["Title"] = "Edit";
}
<h4>Proveedor</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="IdProveedor" />
            <div class="form-group">
                <label asp-for="NombreProveedor" class="control-label"></label>
                <input asp-for="NombreProveedor" class="form-control" />
                <span asp-validation-for="NombreProveedor" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DireccionProveedor" class="control-label"></label>
                <input asp-for="DireccionProveedor" class="form-control" />
                <span asp-validation-for="DireccionProveedor" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TelefonoProveedor1" class="control-label"></label>
                <input asp-for="TelefonoProveedor1" class="form-control" />
                <span asp-validation-for="TelefonoProveedor1" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TelefonoProveedor2" class="control-label"></label>
                <input asp-for="TelefonoProveedor2" class="form-control" />
                <span asp-validation-for="TelefonoProveedor2" class="text-danger"></span>
            </div>
            <div class="form-group py-3">
                <input type="submit" value="Guardar Cambios" class="btn btn-primary" />
                <a class="px-3" asp-action="Index">Cancelar</a>
            </div>
        </form>
    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
